{
  "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest/globals": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 12,
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "consistent-return": "off",
      "import/no-default-export": "error",
      "import/prefer-default-export": "off",
      "no-console": "off"
  },
  "plugins": [
      "jest"
  ]
}
